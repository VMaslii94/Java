<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="DataProvider and listener" parallel="false" thread-count="3">
  <listeners>
    <listener class-name="mentorship.core.Loger.CustomLoger"/>
    <listener class-name="mentorship.core.Reporters.CustomReporter"/>
  </listeners>
  <test name="Login">
    <classes>
      <class name="tests.user.LoginTest"/>
    </classes>
  </test>
  <test name="Account">
    <classes>
      <class name="tests.user.AccountTest"/>
    </classes>
  </test>
</suite>
